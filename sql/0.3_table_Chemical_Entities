\c grottoobs;

CREATE TABLE public.t_chimical_entities (
    name character varying(80) NOT NULL,
    url  character varying(80) NOT NULL,
    definition text
     );

ALTER TABLE ONLY public.t_chimical_entities
    ADD CONSTRAINT t_chimical_entities_pkey PRIMARY KEY (name);

